[ucc/session/remote/session/timeout]
Description[de]=Die UCC Remote-Session wird nach TIMOUT (*3) Sekunden beendet, falls der Server nicht erreichbar ist (default: 100).
Description[en]=Terminate ucc remote session after TIMOUT (*3) seconds if terminal server is not available (default: 100).
Type=int
Categories=system-desktop

[ucc/session/remote/connection/timeout]
Description[de]=Timeout in Sekunden für den Verbindungsaufbau zum UCC Terminal-Server (default: 15)
Description[en]=Timeout in seconds for the UCC terminal server connection (default: 15)
Type=int
Categories=system-desktop

[ucc/session/remote/command]
Description[de]=Dieses Kommando wird bei einer UCC Remote-Session auf dem Terminal-Server ausgeführt (default: /usr/share/univention-ucc-application-server/KDE)
Description[en]=Command to run on the terminal server for a UCC remote session (default: /usr/share/univention-ucc-application-server/KDE)
Type=string
Categories=system-desktop

[ucc/session/remote/disable-sound]
Description[de]=Deaktiviert die Sound-Weiterleitung mit pulseaudio für UCC Terminal-Server Sessions falls auf true gesetzt (default: none)
Description[en]=If set to true sound redirection with pulseaudio in a UCC terminal server session is disabled (default: none)
Type=bool
Categories=system-desktop
